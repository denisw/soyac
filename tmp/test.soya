
class A
{
    var foo : int;

    constructor(foo : int)
    {
        this.foo = foo;
    }

    function printFoo()
    {
        print(foo);
    }
}


class B extends A
{
    var bar : double;

    constructor(foo : int, bar : double)
    : super(foo)
    {
        this.bar = bar;
    }

    function printFooAndBar()
    {
        print(foo);
        print(bar);
    }
}


var a : A = new B(1, 218.89);
a.printFoo();
