extern function exit(code : int);

class Point
{
    var x : int;
    var y : int = 218;
    var z : int;

    constructor()
    {
        this.z = 2;
    }
}

var p = new Point();

if (p.x != 0) exit(1);
if (p.y != 218) exit(2);
if (p.z != 2) exit(3);

