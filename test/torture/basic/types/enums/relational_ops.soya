extern function exit(code : int);

enum Foo
{
    FIRST,
    SECOND,
    THIRD
}

var f = Foo.SECOND;

if (f == Foo.FIRST) exit(1);
if (f != Foo.SECOND) exit(2);
if (f == Foo.THIRD) exit(3);

