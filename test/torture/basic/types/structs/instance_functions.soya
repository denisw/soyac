extern function exit(code : int);

struct Point
{
    var x : int;
    var y : int;

    function getDoubleY() : int
    {
        return y * 2;
    }
    
    function setX(x : int)
    {
        this.x = x;
    }
}

var p = new Point();

p.y = 218;
if (p.getDoubleY() != 436) exit(1);

p.setX(812);
if (p.x != 812) exit(2);

